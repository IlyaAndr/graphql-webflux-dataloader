type Customer {
  id: ID!
  firstName: String!
  lastName: String!
  company: Company!
  outOfOfficeDelegate: Customer
}

type Company {
  id: ID!
  name: String!
  address: String!
  primaryContact: Customer
}

type Query {
  customers: [Customer]
  customersByIds(ids: [Int]!): [Customer]
  companies: [Company]
}

schema {
  query: Query
}
